{
  "name": "micro-business-parse-server-common-react-native",
  "version": "0.1.16",
  "description": "Micro Business Parse Server Common React Native",
  "main": "./dist/index.js",
  "scripts": {
    "start": "babel-node src/index.js",
    "build": "babel src --out-dir dist",
    "lint": "eslint --ext .jsx --ext .js src",
    "fixlint": "eslint --fix --ext .jsx --ext .js src",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "test-verbose": "jest --verbose",
    "test-verbose:watch": "npm run test-verbose -- --watch",
    "publish-package": "npm run build && npm publish",
    "flow": "flow",
    "lint:staged": "lint-staged"
  },
  "author": "Micro Business ltd<micro.business.ltd@gmail.com>",
  "homepage": "https://bitbucket.org/microbusiness/micro-business-parse-server-common-react-native#readme",
  "dependencies": {
    "immutable": "^3.8.1",
    "parse": "git+https://github.com/allmaxgit/Parse-SDK-JS.git",
    "redux-saga": "^0.15.3",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "flow-bin": "^0.47.0",
    "jest": "^20.0.4",
    "lint-staged": "^3.5.0",
    "prettier": "^1.3.1",
    "uuid": "^3.0.1"
  },
  "jest": {
    "rootDir": "./src"
  },
  "pre-commit": "lint:staged",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org:microbusiness/micro-business-parse-server-common-react-native.git"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --single-quote true --trailing-comma all --print-width 150",
      "git add"
    ]
  }
}
